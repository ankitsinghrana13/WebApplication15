@model IEnumerable<WebApplication15.Models.Employee>
@{
}
<a asp-action="Create" asp-controller="Employee" class="btn btn-outline-danger">Create</a>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Mobile</th>
            <th>Address</th>

        </tr>
    </thead>
    @foreach(var emp in Model)
            {
<tr>
    <td>@emp.Id</td>
    <td>@emp.Name</td>
    <td>@emp.Email</td>
    <td>@emp.Mobile</td>
    <td>@emp.Address</td>
    <td>

    <a asp-action="Edit" asp-controller="Employee" asp-route-id="@emp.Id" class="btn btn-success">Edit</a>
    <a asp-action="Delete" asp-controller="Employee" asp-route-id="@emp.Id" class="btn btn-danger">Delete</a></td>
</tr>
            }

</table>
